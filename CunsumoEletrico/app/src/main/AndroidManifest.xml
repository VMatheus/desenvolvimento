<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.matheus.cunsumoeletrico">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PrincipalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdicionarComodoActivity"
            android:label="@string/title_activity_adicionar_comodo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AdicionarAparelhoActivity"
            android:label="@string/title_activity_adicionar_aparelho"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ListAparelhosGeraisActivity"
            android:label="@string/title_activity_list_aparelhos"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>